{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"IngestionDelay": 10,
	"MaxFileSize": 209715200,
	"MinRelevance": 0.25,
	"ProductVersion": "1.13",
	"DbConnectionTimeout": 300,
	"DbServer": "host.docker.internal",
	"DbPort": 5439,
	"DbName": "<database-name>",
	"DbUser": "<database-username>",
	"DbPassword": "<database-user-password>",
